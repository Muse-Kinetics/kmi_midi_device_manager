/* ---------- QSpinBox ------------*/

QSpinBox
{
        font: 10pt "Open Sans";
        background-color: rgba(1, 1, 1, 100);
        color: red;

       padding-left: 4px;
                border: 1px solid rgb(100,0,0);
}

QAbstractSpinBox:focus
{
        background-color: rgba(60, 60, 60, 100);
        color: white;
        outline: none;
}


QSpinBox::up-button {
    width: 16px; /* Adjust size as needed */
    height: 9px; /* Adjust size as needed */
        padding: 0 0;
    subcontrol-origin: border;
    subcontrol-position: top right; /* Position can be adjusted if needed */
    image: url(:/ui/ui_images/arrow-up.svg);
}


QSpinBox::down-button {
    width: 16px; /* Adjust size as needed */
    height: 9px; /* Adjust size as needed */
   padding: 0 0;
    subcontrol-origin: border;
    subcontrol-position: bottom right; /* Position can be adjusted if needed */
    image: url(:/ui/ui_images/arrow-down.svg);
}


QAbstractSpinBox:focus
{
        background-color: rgba(60, 60, 60, 100);
        color: white;
        outline: none;
}



