 }

/*----------- QComboBox ------------*/

/* dropdown box*/
QComboBox QListView
{
    border: 1px solid rgb(255,255,255);
        color: white;
        font: 9pt "Open Sans";
        padding-left: 3px;
        padding-right: 3px;
        min-width: 50px;
    height: 200px;
}

/*----------- dropdown items hovered ------------*/
QComboBox QAbstractItemView
{
    background-color: rgb(40, 40, 40);
    selection-color: white;
    selection-background-color: rgb(0,174,239);
    min-width: 50px;
}

QComboBox::item:alternate
{
        background: rgb(90, 90, 90);
}

QComboBox::item:selected
{
        border: 1px solid transparent;
        background: rgb(0,174,239);
}

QComboBox::indicator
{
        background-color: transparent;
        selection-background-color: transparent;
        color: transparent;
        selection-color: white;
}

/* down arrow image */
QComboBox::down-arrow
{
    border: none;
    background: none;
    image: url(:/ui/ui_images/combobox_menu_arrows.svg);
    padding-right: 12px;
    width: 12px;
    height: 12px;
}

/* required for down-arrow or it's blank*/
QComboBox::drop-down
{
    width: 1px;
    height: 1px;
    subcontrol-origin: margin;
    subcontrol-position: right center;
    border-left-width: 0px;
    border-right-width: 0px;
    border-top-width: 0px;
    border-bottom-width: 0px;
        padding: 1px 1px 1px 4px;
}

/* down arrow image */
QComboBox::down-arrow
{
        border: none;
        background: none;
        image: url(:/ui/ui_images/combobox_menu_arrows.svg);
        padding-right: 15px;
}

/* ---[scroll bar]----------------------------------------------------------------------------*/

QComboBox QAbstractItemView QScrollBar:vertical {
        background: black;
/* margin sets up how far the handle can travel */
        margin: 0px 0px 0px 0px;
        border: none;
}

/* Sets up the color and height of handle */
QComboBox QAbstractItemView QScrollBar::handle:vertical {
        background: rgb(255,255,255);
        border: none;
}

/* This removes the bottom button by setting the height to 0px */
QComboBox QAbstractItemView QScrollBar::add-line:vertical {
        height: 0px;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
        border: none;
}

/* This remvoes the top button by setting the height to 0px */
QComboBox QAbstractItemView QScrollBar::sub-line:vertical {
        height: 0px;
        subcontrol-position: top;
        subcontrol-origin: margin;
        border: none;
}

/* need this to get rid of crosshatching on scrollbar background */
QComboBox QAbstractItemView QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
        background: none;
        border: none;
}


